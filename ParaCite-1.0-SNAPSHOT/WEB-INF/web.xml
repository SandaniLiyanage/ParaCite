<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <servlet>
    <servlet-name>FileUploadHandler</servlet-name>
    <servlet-class>com.mycompany.paracite.FileUploadHandler</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginHandler</servlet-name>
    <servlet-class>com.mycompany.paracite.LoginHandler</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SignupHandler</servlet-name>
    <servlet-class>com.mycompany.paracite.SignupHandler</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogoutHandler</servlet-name>
    <servlet-class>com.mycompany.paracite.LogoutHandler</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>WebSearchHandler</servlet-name>
    <servlet-class>com.mycompany.paracite.WebSearchHandler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileUploadHandler</servlet-name>
    <url-pattern>/FileUploadHandler</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginHandler</servlet-name>
    <url-pattern>/LoginHandler</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SignupHandler</servlet-name>
    <url-pattern>/SignupHandler</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutHandler</servlet-name>
    <url-pattern>/logout.jspx</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>WebSearchHandler</servlet-name>
    <url-pattern>/WebSearchHandler</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>